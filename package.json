{
  "scripts": {
    "startOLD": "npm-run-all --parallel css eleventy",
    "eleventyOLD": "eleventy --serve",
    "debugOLD": "set DEBUG=* & eleventy",
    "css": "postcss src/static/css/tailwind.css --o _tmp/static/css/style.css --watch",
    "buildOLD": "cross-env NODE_ENV=production eleventy && cross-env NODE_ENV=production postcss src/static/css/tailwind.css --o _site/static/css/style.css",
    "build": "cross-env NODE_ENV=production npm-run-all clean -p build:*",
    "build:11ty": "eleventy --quiet",
    "build:rollup": "rollup -c",
    "clean": "rimraf dist",
    "start": "npm-run-all clean build:* -p dev:*",
    "dev:11ty": "eleventy --serve --quiet",
    "dev:rollup": "rollup -c -w",
    "dev:cms": "npx netlify-cms-proxy-server",
    "bs": "cross-env NODE_ENV=production npm-run-all clean -p build:* serve",
    "serve:": "npx serve dist"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@tailwindcss/typography": "^0.3.1",
    "alpinejs": "^2.6.0",
    "autoprefixer": "^10.1.0",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "html-minifier": "^4.0.0",
    "javascript-stringify": "^2.0.1",
    "js-yaml": "^3.14.0",
    "luxon": "^1.25.0",
    "moment": "^2.29.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.2",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-nested": "^5.0.3",
    "prettier": "^2.2.1",
    "prismjs": "^1.21.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^2.0.2",
    "tailwindcss-debug-screens": "^2.0.0",
    "util": "^0.12.3"
  },
  "dependencies": {
    "netlify-cms-proxy-server": "^1.3.8"
  }
}
